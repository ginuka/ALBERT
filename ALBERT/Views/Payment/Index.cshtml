@model List<Payment>

<h3>All Payments</h3>
<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Amount</th>
            <th>Payment Method</th>
            <th>Status</th>
            <th>Transaction ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var payment in Model)
        {
            <tr>
                <td>@payment.OrderId</td>
                <td>@payment.Amount</td>
                <td>@payment.PaymentMethod</td>
                <td>@payment.Status</td>
                <td>@payment.TransactionId</td>
            </tr>
        }
    </tbody>
</table>
